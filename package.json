{
  "name": "uniq",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "jsnext:main": "build/index.es.js",
  "scripts": {
    "install": "./node_modules/.bin/rollup -c",
    "test": "jest",
    "mongod": "mongod --dbpath ./uniq-server/db --replSet rs0",
    "prod-build": "npm run build --prefix ./uniq-web && npm run build --prefix ./uniq-server",
    "prod-server": "npm run prod --prefix ./uniq-server -- -s ../uniq-web/build -p",
    "dev-web": "npm run start --prefix ./uniq-web",
    "dev-server": "npm run dev --prefix ./uniq-server"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "uniq-web",
      "uniq-server",
      "build",
      "node_modules"
    ]
  },
  "author": "Brandon Kelley",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=15.0.1",
    "react-dom": ">=15.0.1"
  },
  "dependencies": {
    "rollup": "^2.32.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-typescript2": "^0.28.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/react": "^16.9.53",
    "jest": "^26.5.2",
    "react": ">=15.0.1",
    "react-dom": ">=15.0.1",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "files": ["build", "rollup.config.js"]
}
